<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.mobilemovieplaza739079" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>mobileMoviePlaza</name>
  <description>
       A Mobile Project For Movie API based on Spring.
    </description>
  <author email="bhavindesai20@gmail.com" href="">
      Bhavinkumar Desai
    </author>
  <content src="index.html"/>
  <access origin="*"/>
